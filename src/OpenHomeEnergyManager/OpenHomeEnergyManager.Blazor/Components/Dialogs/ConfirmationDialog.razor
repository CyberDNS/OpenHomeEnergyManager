<MudDialog  DisableSidePadding="true" style="height: 500px; width: 900px;">
    <DialogContent>
        <MudContainer Style="max-height: 300px; width: 100%; overflow-y: scroll">
            @DialogMessage
        </MudContainer>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>
